[
  {
    "action": "data",
    "src": ["Anpr tutorial/artificial"],
    "dst": "$sample0",
    "settings": {
      "classes_mapping":{
        "licence plate":"plate",
        "plate":"plate"
      }
    }
  },
  {
    "action": "if",
    "src": ["$sample0"],
    "dst": ["$sample_train", "$sample_test"],
    "settings": {
      "condition": {
        "probability": 0.98
      }
    }
  },
  {
    "action": "bbox",
    "src": ["$sample_train"],
    "dst": "$sample_train_bb",
    "settings": {
      "rasterize": false,
      "classes": ["plate"]
    }
  },
  {
    "action": "bbox",
    "src": ["$sample_test"],
    "dst": "$sample_test_bb",
    "settings": {
      "rasterize": false,
      "classes": ["plate"]
    }
  },
  {
    "action": "save",
    "src": ["$sample_train"],
    "dst": "artificial_train",
    "settings": {
      "images": true,
      "annotations": true
    }
  },
  {
    "action": "save",
    "src": ["$sample_test"],
    "dst": "artificial_test",
    "settings": {
      "images": true,
      "annotations": true
    }
  }
]