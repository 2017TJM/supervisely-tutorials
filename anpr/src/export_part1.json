[
  {
    "action": "data",
    "src": [
      "input/input"
    ],
    "dst": "$sample01",
    "settings": {
      "classes_mapping": "default"
    }
  },
  {
    "action": "resize",
    "src": [
      "$sample01"
    ],
    "dst": "$sample0",
    "settings": {
      "aspect_ratio": {
        "keep": false
      },
      "width": 128,
      "height": 64
    }
  },
  {
    "action": "bbox",
    "src": [
      "$sample0"
    ],
    "dst": "$sample_bb",
    "settings": {
      "rasterize": false,
      "classes": [
        "receipt"
      ]
    }
  },
  {
    "action": "if",
    "src": [
      "$sample_bb"
    ],
    "dst": [
      "$sample_train",
      "$sample_test"
    ],
    "settings": {
      "condition": {
        "probability": 0.8
      }
    }
  },
  {
    "action": "tag",
    "src": [
      "$sample_train"
    ],
    "dst": "$train_tagged",
    "settings": {
      "tag": "train",
      "action": "add"
    }
  },
  {
    "action": "tag",
    "src": [
      "$sample_test"
    ],
    "dst": "$test_tagged",
    "settings": {
      "tag": "test",
      "action": "add"
    }
  },
  {
    "action": "save",
    "src": [
      "$train_tagged",
      "$test_tagged"
    ],
    "dst": "artificial_train",
    "settings": {
      "images": true,
      "annotations": true
    }
  }
]